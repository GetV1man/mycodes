#include <stdio.h>

int main() {
    // Введення довжини сторін трикутника
    int a, b, c;

    // Перевірка чи існує трикутник з такими сторонами
    if (a <= 0 || b <= 0 || c <= 0 || a + b <= c || a + c <= b || b + c <= a) {
        printf("Такого трикутника не існує\n");
    }
    else {
        // Визначення типу трикутника
        if (a == b && b == c) {
            printf("Рівносторонній трикутник\n");
        }
        else if (a == b || b == c || c == a) {
            printf("Рівнобедрений трикутник\n");
        }
        else {
            printf("Різносторонній трикутник\n");
        }
    }

    return 0;
}
