#include <stdio.h>

int main() {
    // Вказані відрізки
    int a, b, c;

    // Перевірка чи можна побудувати трикутник
    if (a + b > c && a + c > b && b + c > a) {
        printf("Трикутник можна побудувати\n");
    } else {
        printf("Трикутник не можна побудувати\n");
    }

    return 0;
}
